[[workflows.workflow]]
name = "Run Bot"
author = 40502297
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node src/index.js"

[[workflows.workflow]]
name = "GitHub Editor"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd /home/runner/workspace/github-tools && node minimal-github-editor.js"

[[workflows.workflow]]
name = "Terminal QR Connect"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd /home/runner/workspace && node terminal-qr-connect.js"

[[workflows.workflow]]
name = "High Performance Bot"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node high-performance-whatsapp.js"

[[workflows.workflow]]
name = "WhatsApp Bot"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node src/core/index.js"
waitForPort = 5000

[deployment]
run = ["sh", "-c", "cd /home/runner/workspace && node src/qr-web-server-fixed.js"]

