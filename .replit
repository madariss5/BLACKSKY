modules = ["nodejs-20", "python-3.11"]

[nix]
channel = "stable-24_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "GitHub Editor"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Terminal QR Connect"

[[workflows.workflow.tasks]]
task = "workflow.run"
