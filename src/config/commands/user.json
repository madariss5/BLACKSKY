{
  "category": "user",
  "commands": [
    {
      "name": "register",
      "description": "Register your user profile",
      "usage": ".register [name] [age]",
      "example": ".register John 25",
      "cooldown": 0,
      "permissions": ["user"]
    },
    {
      "name": "profile",
      "description": "View user profile",
      "usage": ".profile [@user]",
      "example": ".profile @user",
      "cooldown": 5,
      "permissions": ["user"]
    },
    {
      "name": "setbio",
      "description": "Set your profile bio",
      "usage": ".setbio [text]",
      "example": ".setbio Hello World!",
      "cooldown": 300,
      "permissions": ["user"]
    },
    {
      "name": "daily",
      "description": "Claim your daily rewards",
      "usage": ".daily",
      "example": ".daily",
      "cooldown": 86400,
      "permissions": ["user"]
    }
  ]
}