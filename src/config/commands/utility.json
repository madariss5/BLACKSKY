{
  "category": "utility",
  "commands": [
    {
      "name": "calculate",
      "description": "Calculator with advanced mathematical functions",
      "usage": ".calculate [expression]",
      "example": ".calculate 2 + 2 * 3",
      "cooldown": 3,
      "permissions": ["user"]
    },
    {
      "name": "encode",
      "description": "Encode text (base64, hex, binary)",
      "usage": ".encode [type] [text]",
      "example": ".encode base64 Hello World",
      "cooldown": 5,
      "permissions": ["user"]
    },
    {
      "name": "decode",
      "description": "Decode encoded text",
      "usage": ".decode [type] [text]",
      "example": ".decode base64 SGVsbG8gV29ybGQ=",
      "cooldown": 5,
      "permissions": ["user"]
    },
    {
      "name": "poll",
      "description": "Create interactive polls",
      "usage": ".poll [question] | [option1] | [option2] ...",
      "example": ".poll Best color? | Red | Blue | Green",
      "cooldown": 30,
      "permissions": ["user"]
    },
    {
      "name": "todo",
      "description": "Manage personal todo list",
      "usage": ".todo <add|remove|list> [item]",
      "example": ".todo add Buy groceries",
      "cooldown": 5,
      "permissions": ["user"]
    },
    {
      "name": "notes",
      "description": "Manage personal notes",
      "usage": ".notes <add|view|delete> [content/note_number]",
      "example": ".notes add Remember to call mom",
      "cooldown": 5,
      "permissions": ["user"]
    },
    {
      "name": "reverse",
      "description": "Reverse provided text",
      "usage": ".reverse [text]",
      "example": ".reverse Hello World",
      "cooldown": 5,
      "permissions": ["user"]
    },
    {
      "name": "mock",
      "description": "Convert text to Spongebob mocking style",
      "usage": ".mock [text]",
      "example": ".mock hello there",
      "cooldown": 5,
      "permissions": ["user"]
    },
    {
      "name": "roll",
      "description": "Roll a dice with specified number of sides",
      "usage": ".roll [sides]",
      "example": ".roll 20",
      "cooldown": 3,
      "permissions": ["user"]
    },
    {
      "name": "flip",
      "description": "Flip a coin",
      "usage": ".flip",
      "example": ".flip",
      "cooldown": 3,
      "permissions": ["user"]
    },
    {
      "name": "choose",
      "description": "Choose randomly between provided options",
      "usage": ".choose [option1] | [option2] | ...",
      "example": ".choose Pizza | Burger | Salad",
      "cooldown": 5,
      "permissions": ["user"]
    }
  ]
}